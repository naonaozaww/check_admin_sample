<tr>
  <td><%= user.id %></td>
  <td><%= user.first_name %></td>
  <td><%= user.last_name %></td>
  <td><%= user.role %></td>
  <td><%= user.email %></td>
  <td><%= user.created_at %></td>
  <td>
    <%= link_to 'Show', admin_user_path(user), class: 'btn btn-info' %>
    <%= link_to 'Edit', edit_admin_user_path(user), class: 'btn btn-success' %>
    <%= link_to 'Delete', admin_user_path(user), class: 'btn btn-danger', method: :delete, data: { confirm: 'Are you sure?' } %>
  </td>
</tr>
